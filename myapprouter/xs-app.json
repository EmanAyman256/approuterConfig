
{
    "authenticationMethod": "route",
    "routes": [
      {
        "source": "^/(.*)$",
        "target": "$1",
        "destination": "sd-app",
        "authenticationType": "xsuaa"
      },
      {
        "source": "^/api/(.*)$",
        "target": "$1",
        "destination": "btpsd",
        "csrfProtection": false,
        "authenticationType": "xsuaa"
      }
    ]

}